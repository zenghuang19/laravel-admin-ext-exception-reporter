{
    "name": "exception-reporter/laravel-admin-ext",
    "description": "Exception reporter for laravel",
    "type": "library",
    "keywords": ["laravel-admin", "exception", "reporter"],
    "homepage": "https://github.com/zenghuang19/laravel-admin-ext-exception-reporter.git",
    "license": "MIT",
    "authors": [
        {
            "name": "zeng",
            "email": "zenghuang19@163.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "~6.0",
        "encore/laravel-admin": "~1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "laravel/laravel": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Encore\\Admin\\Reporter\\": "src/"
        }
